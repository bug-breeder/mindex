  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:mobile": "vitest src/__tests__/mobile-responsiveness.test.tsx",
    "test:folders": "vitest src/components/folders src/api/__tests__/folders.test.ts",
    "test:integration": "vitest src/__tests__/folder-management-integration.test.tsx",
    "test:interactions": "vitest src/__tests__/user-interactions.test.tsx",
    "test:components": "vitest src/components",
    "test:api": "vitest src/api",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "vitest related --run --passWithNoTests"
    ]
  },
